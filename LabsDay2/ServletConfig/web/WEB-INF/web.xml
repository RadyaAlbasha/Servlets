<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>appName</param-name>
        <param-value>GoFun</param-value>
    </context-param>
    <servlet>
        <servlet-name>Configration1</servlet-name>
        <servlet-class>myServlet.SetterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Configration2</servlet-name>
        <servlet-class>myServlet.GetterServlet</servlet-class>
        <init-param>
            <param-name>userName</param-name>
            <param-value>Radya</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Word</servlet-name>
        <servlet-class>myServlet.MimeTypeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Configration1</servlet-name>
        <url-pattern>/config1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Configration2</servlet-name>
        <url-pattern>/config2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Word</servlet-name>
        <url-pattern>/word</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
